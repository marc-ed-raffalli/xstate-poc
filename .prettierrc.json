{
  "jsxSingleQuote": false,
  "singleQuote": true,
  "printWidth": 100,
  "quoteProps": "as-needed",
  "proseWrap": "preserve",
  "arrowParens": "always",
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ["*.{ts,tsx}"],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
